{
  "name": "searchneu",
  "version": "1.0.9",
  "scripts": {
    "_comment": "These commands are used in dev.",
    "start": "cross-env NODE_ENV=dev babel-node backend/server.js",
    "scrape": "babel-node --max_old_space_size=12000 backend/scrapers/main.js",
    "scrape_classes": "babel-node --max_old_space_size=12000 backend/scrapers/classes/main.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js backend/ common/ frontend/",
    "fix": "node ./node_modules/eslint/bin/eslint.js backend/ common/ frontend/ --fix",
    "test": "./node_modules/jest-cli/bin/jest.js",
    "coverage": "./node_modules/jest-cli/bin/jest.js --coverage",
    "_comment2": "These comands are used to build the code",
    "build_frontend": "mkdir -p public; cp frontend/static/* public/; cross-env NODE_ENV=prod babel-node ./node_modules/webpack/bin/webpack.js --config ./backend/webpack.config.babel.js --profile --colors",
    "build_backend": "mkdir -p dist; mkdir -p dist/backend; ./node_modules/babel-cli/bin/babel.js backend -d dist/backend; ./node_modules/babel-cli/bin/babel.js common -d dist/common",
    "build": "npm run build_backend; npm run build_frontend",
    "start_prod": "./node_modules/forever/bin/forever stopall; NODE_ENV=prod ./node_modules/forever/bin/forever -a -l forever.log -o log.log -e error.log start dist/backend/startup.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanhugh/searchneu.git"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-1"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-object-rest-spread",
      "react-hot-loader/babel"
    ]
  },
  "main": "dist/backend/scrapers/main.js",
  "_comment": "domutils upgrade is faster for parsing html (stack instead of recursive) but requires some code changes. Shoudn't be too bad.",
  "_comment2": "Async adds about 30k to the final bundled output. There are smaller libraries that can also do async.retry (only thing that is being used in async)",
  "_comment3": "but that is only about 3% of the final JS size.",
  "dependencies": {
    "@mapbox/togeojson": "^0.16.0",
    "acorn": "^5.0.3",
    "amplitude": "^3.5.0",
    "async": "^2.1.5",
    "atob": "^2.0.3",
    "babel-core": "^6.21.0",
    "babel-jest": "^22.0.4",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.17.2",
    "cheerio": "^0.22.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cookie": "^0.3.1",
    "cross-env": "^5.0.0",
    "deep-diff": "^0.3.8",
    "dnscache": "^1.0.1",
    "domutils": "^1.6.2",
    "elasticlunr": "^0.9.5",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint-loader": "^1.9.0",
    "express": "^4.15.2",
    "express-async-wrap": "^1.0.0",
    "express-x-hub": "^1.0.4",
    "file-loader": "^1.1.5",
    "firebase-admin": "^5.5.1",
    "forever": "^0.15.3",
    "form-urlencoded": "^2.0.4",
    "fs-extra": "^5.0.0",
    "he": "^1.1.1",
    "html-webpack-plugin": "^2.28.0",
    "htmlparser2": "^3.9.2",
    "identity-obj-proxy": "^3.0.0",
    "is-mobile": "^0.2.2",
    "jest": "^22.0.4",
    "json": "^9.0.6",
    "lodash": "^4.17.4",
    "mkdirp-promise": "^5.0.1",
    "mock-local-storage": "^1.0.5",
    "mockdate": "^2.0.1",
    "moment": "^2.17.1",
    "msgpack5": "^4.0.0",
    "object-hash": "^1.1.7",
    "prop-types": "^15.5.8",
    "randomstring": "^1.1.5",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-test-renderer": "^16.1.1",
    "react-tooltip": "^3.2.6",
    "react-transition-group": "^2.2.1",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "resolve-url-loader": "^2.0.2",
    "rollbar": "^2.1.3",
    "semantic-ui-css": "^2.2.4",
    "semantic-ui-react": "^0.77.1",
    "semver": "^5.4.1",
    "urijs": "^1.18.8",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.16.1",
    "whois": "^2.3.9",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-runtime": "^6.20.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.28.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "ignore-styles": "^5.0.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "setupFiles": [
      "./common/rafTestPolyfill.js"
    ],
    "moduleNameMapper": {
      "__comment": "The css import needs to be an object that returns a string",
      "__comment2": "but the svg and png imports need to be string themselves.",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.(svg|png)$": "<rootDir>/frontend/components/tests/exportsAString.js"
    }
  }
}
